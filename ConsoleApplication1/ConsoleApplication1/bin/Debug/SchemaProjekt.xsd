<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">
  <xs:element name="trasy">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="trasa">
          <xs:complexType>
            <xs:sequence>
              <xsd:attribute name="id_trasy"type="xsd:ID" use="required"/>
              <xs:element name="punkt_poczatkowy">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="id_punktu" type="xs:integer"/>
                    <xs:element name="nazwa_punktu" type="xs:string"/>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="punkt_koncowy">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="id_punktu" type="xs:integer"/>
                    <xs:element name="nazwa_punktu" type="xs:string"/>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="ilosc_punktow" type="xs:integer"/>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
</xs:schema>
